{
  "name": "chatgpt-vue",
  "version": "0.0.1",
  "author": "LiangINX",
  "main": "index.html",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "e:dev": "yarn build && cp -r electron/. dist && cd dist && yarn && yarn dev",
    "e:build": "yarn build && cp -r electron/. dist && cd dist && yarn && yarn build",
    "ne:dev": "npm run build && cp -r electron/. dist && cd dist && npm run && npm run dev",
    "ne:build": "npm run build && cp -r electron/. dist && cd dist && npm run && npm run build",
    "start": "yarn && yarn build"
  },
  "dependencies": {
    "@icon-park/vue-next": "^1.4.2",
    "crypto-js": "^4.1.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.12",
    "axios": "^1.3.4",
    "express": "^4.18.2",
    "nodemon": "^2.0.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0"
  }
}